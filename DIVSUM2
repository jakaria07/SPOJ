#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define ull unsigned long long
#define val 1000005
#define MOD 1000000007
#define SIZE 100000005


#define BOOST               ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define sf                  scanf
#define pf                  printf

const ll sz = 1e8+5;

bitset<sz> sieve;
vector<ll> prime;

void primeSieve()
{
    sieve[0] = sieve[1] = 1;
    prime.push_back(2);
    for(ll i = 4; i <= sz; i += 2)
    {
        sieve[i] = 1;
    }
    for(ll i = 3; i <= sz; i += 2)
    {
        if(sieve[i] == 0)
        {
            for(ll j = i*i; j <= sz; j += (1ll*2*i))
            {
                sieve[j] = 1;
            }
        }
    }
    for(ll i = 3; i <= sz; i += (2*1ll))
    {
        if(sieve[i] == 0)
        {
            prime.push_back(i);
        }
    }
}

ll SOD(ll n)
{
    ll res = 1;
    ll sq = sqrt(n);
    for(ll i = 0; i < prime.size() && prime[i] <= sq; i++)
    {
        if(n%prime[i] == 0)
        {
            ll tempSum = 1;
            ll p = 1;
            while(n%prime[i] == 0)
            {
                n /= prime[i];
                p *= prime[i];
                tempSum += p;
            }
            sq = sqrt(n);
            res *= tempSum;
        }
    }
    if(n != 1)
    {
        res *= (n+1);
    }
    return res;
}

int main()
{
    BOOST;
    primeSieve();
    int t;
    cin >> t;
    while(t--)
    {
        ll n;
        cin >> n;
        ll ans = SOD(n);
        cout << ans-n << endl;
    }

    return 0;
}
